<odoo>
    <record id="view_generate_accounting_entries_wizard" model="ir.ui.view">
        <field name="name">generate.accounting.entries.wizard.form</field>
        <field name="model">generate.accounting.entries.wizard</field>
        <field name="arch" type="xml">
            <form string="Generar Asientos Contables">
                <group>
                    <field name="start_date"/>
                    <field name="end_date"/>
                    <field name="move_type"/>
                </group>
                <group>
                    <field name="stock_move_ids" widget="many2many_tags"/>
                </group>
                <footer>
                    <button string="Generar Asientos" type="object" name="generate_accounting_entries" class="btn-primary"/>
                    <button string="Cancelar" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

</odoo>